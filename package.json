{
    "name": "caf_react_helloworld",
    "description": "Cloud Assistants basic example with React/Redux",
    "version": "0.3.0",
    "author": "Antonio Lain <antlai@cafjs.com>",
    "dependencies": {
        "caf_cli": "^0.3.0",
        "caf_core": "^0.3.0",
        "caf_react": "^0.3.0",
        "caf_transport": "^0.3.0",
        "keymirror": "^0.1.0",
        "object-assign": "^4.1.0",
        "react": "^0.14.0",
        "react-bootstrap": "^0.28.1",
        "react-dom": "^0.14.0",
        "redux": "^3.5.0"
    },
    "devDependencies": {
        "browserify": "^13.0.0",
        "nodeunit": ">=0.9",
        "uglify-es": "^3.3.8"
    },
    "scripts": {
        "start": "node ./index.js",
        "test": "node ./node_modules/.bin/nodeunit",
        "build-js": "browserify  -d public/js/main.js -o public/js/build.js",
        "build-min-js": "export NODE_ENV=production; browserify public/js/main.js | uglifyjs > public/js/build.min.js",
        "build": "yarn run build-js && yarn run build-min-js"
    },
    "main": "index",
    "engines": {
        "node": ">= 8.0.0"
    }
}
